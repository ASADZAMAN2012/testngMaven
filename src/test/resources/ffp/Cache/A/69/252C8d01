      <max_version>25.*</max_version>
      <appID>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</appID>
    </application></compatible_applications>
  <all_compatible_os><os>ALL</os></all_compatible_os>
  <eula></eula><previews><preview position="0">
      <full type="image/png" width="661" height="525">
        https://addons.cdn.mozilla.net/img/uploads/previews/full/50/50494.png?src=api&amp;modified=1331247702
      </full>
